setwd("J:/360/projekt")        #nie byłem pewien czy cały czas działa na odpowiednim miejscu
library("tidyverse")           #install.packages("tidyverse")
library("readxl")              #install.packages("readxl")
library("plotly")              #install.packages("plotly") 
essa=readxl::read_xlsx("dane.xlsx",sheet=1)
koszyk.dolno<-essa[essa$Nazwa == "DOLNOŚLĄSKIE", ]
koszyk.kujawsko<-essa[essa$Nazwa == "KUJAWSKO-POMORSKIE", ]
koszyk.lubel<-essa[essa$Nazwa == "LUBELSKIE", ]
koszyk.lubel1<-essa[essa$Nazwa == "LUBUSKIE", ]
koszyk.lubel11<-essa[essa$Nazwa == "ŁÓDZKIE", ]
koszyk.lubel2<-essa[essa$Nazwa == "MAŁOPOLSKIE", ]
koszyk.lubel22<-essa[essa$Nazwa == "MAZOWIECKIE", ]
koszyk.lubel3<-essa[essa$Nazwa == "OPOLSKIE", ]
koszyk.lubel33<-essa[essa$Nazwa == "PODLASKIE", ]
koszyk.lubel4<-essa[essa$Nazwa == "POMORSKIE", ]
koszyk.lubel44<-essa[essa$Nazwa == "ŚLĄSKIE", ]
koszyk.lubel5<-essa[essa$Nazwa == "ŚWIĘTOKRZYSKIE", ]
koszyk.lubel55<-essa[essa$Nazwa == "WARMIŃSKO-MAZURSKIE", ]
koszyk.lubel6<-essa[essa$Nazwa == "WIELKOPOLSKIE", ]
koszyk.lubel66<-essa[essa$Nazwa == "ZACHODNIOPOMORSKIE", ]
koszyk.lubel7<-essa[essa$Nazwa == "PODKARPACKIE", ]

koszyk.dolno.06<-koszyk.dolno[koszyk.dolno$Rok=="2006",]
vec.koszyk.dolno.06<-c(koszyk.dolno.06$Wartosc)
vec.koszyk.dolno.06<-as.numeric(vec.koszyk.dolno.06)
mean.koszyk.dolno.06<-mean(vec.koszyk.dolno.06, na.rm=TRUE)

koszyk.dolno.07<-koszyk.dolno[koszyk.dolno$Rok=="2007",]
