srednia.koszyk.dolno<-c(mean.koszyk.dolno.06,mean.koszyk.dolno.07,mean.koszyk.dolno.08,mean.koszyk.dolno.09,mean.koszyk.dolno.10,
                       mean.koszyk.dolno.11,mean.koszyk.dolno.12,mean.koszyk.dolno.13,mean.koszyk.dolno.14,
                       mean.koszyk.dolno.15,mean.koszyk.dolno.16,mean.koszyk.dolno.17,mean.koszyk.dolno.18,mean.koszyk.dolno.19)

lata<-c(2006,2007,2008,2009,2010,2011,2012,2013,2014,2015,2016,2017,2018,2019)
png(file = "koszyk.dolno.jpg")
plot(srednia.koszyk.dolno,type = "o", col = "red", xlab='', ylab = "",ylim=c(10,25),
     main = "Średnia cena za zakupy w Dolnośląskim",xaxt = 'n')
lablist<-as.vector(c(2006:2019))
axis(1, at=seq(1, 14, by=1), labels = FALSE)
text(seq(1, 14, by=1), par("usr")[3] - 0.2, labels = lablist, srt = 45, pos = 1, xpd = TRUE)
dev.off()

